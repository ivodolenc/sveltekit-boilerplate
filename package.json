{
  "name": "sveltekit-boilerplate",
  "version": "0.4.0",
  "author": "Ivo Dolenc",
  "description": "SvelteKit starter template.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ivodolenc/sveltekit-boilerplate.git"
  },
  "homepage": "https://github.com/ivodolenc/sveltekit-boilerplate",
  "private": true,
  "type": "module",
  "keywords": [
    "svelte",
    "sveltekit",
    "boilerplate",
    "template",
    "starter"
  ],
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "sync": "svelte-kit sync",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "format": "prettier --write ."
  },
  "packageManager": "pnpm@10.7.1",
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=10.0.0"
  },
  "devDependencies": {
    "@sveltejs/adapter-static": "^3.0.8",
    "@sveltejs/kit": "^2.20.3",
    "@sveltek/eslint-config": "^1.0.0",
    "@sveltek/prettier-config": "^1.0.2",
    "@tailwindcss/vite": "^4.1.1",
    "@types/node": "^22.14.0",
    "eslint": "^9.23.0",
    "prettier": "^3.5.3",
    "svelte": "^5.25.6",
    "svelte-check": "^4.1.5",
    "tailwindcss": "^4.1.1",
    "typescript": "^5.8.2",
    "vite": "^6.2.5"
  }
}
