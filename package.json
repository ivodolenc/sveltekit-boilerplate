{
  "name": "sveltekit-boilerplate",
  "version": "0.3.0",
  "author": "Ivo Dolenc",
  "description": "SvelteKit starter template.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ivodolenc/sveltekit-boilerplate.git"
  },
  "homepage": "https://github.com/ivodolenc/sveltekit-boilerplate",
  "private": true,
  "type": "module",
  "keywords": [
    "svelte",
    "sveltekit",
    "boilerplate",
    "template",
    "starter"
  ],
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "sync": "svelte-kit sync",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "format": "prettier --write ."
  },
  "packageManager": "pnpm@10.5.2",
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=9.0.0"
  },
  "devDependencies": {
    "@hypernym/eslint-config": "^3.5.8",
    "@hypernym/prettier-config": "^3.2.3",
    "@sveltejs/adapter-static": "^3.0.8",
    "@sveltejs/kit": "^2.17.3",
    "@sveltejs/vite-plugin-svelte": "^5.0.3",
    "@tailwindcss/vite": "^4.0.9",
    "@types/node": "^22.13.5",
    "eslint": "^9.21.0",
    "eslint-plugin-svelte": "^3.0.2",
    "prettier": "^3.5.2",
    "prettier-plugin-svelte": "^3.3.3",
    "svelte": "^5.20.5",
    "svelte-check": "^4.1.4",
    "svelte-eslint-parser": "^1.0.0",
    "tailwindcss": "^4.0.9",
    "typescript": "^5.7.3",
    "vite": "^6.2.0"
  }
}
