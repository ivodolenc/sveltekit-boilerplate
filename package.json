{
  "name": "sveltekit-boilerplate",
  "version": "0.4.2",
  "author": "Ivo Dolenc",
  "description": "SvelteKit starter template.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ivodolenc/sveltekit-boilerplate.git"
  },
  "homepage": "https://github.com/ivodolenc/sveltekit-boilerplate",
  "private": true,
  "type": "module",
  "keywords": [
    "svelte",
    "sveltekit",
    "boilerplate",
    "template",
    "starter"
  ],
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "sync": "svelte-kit sync",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "format": "prettier --write ."
  },
  "devDependencies": {
    "@sveltejs/adapter-static": "^3.0.8",
    "@sveltejs/kit": "^2.21.0",
    "@sveltek/eslint-config": "^1.1.0",
    "@sveltek/prettier-config": "^1.0.3",
    "@tailwindcss/vite": "^4.1.6",
    "@types/node": "^22.15.18",
    "eslint": "^9.26.0",
    "prettier": "^3.5.3",
    "svelte": "^5.30.1",
    "svelte-check": "^4.2.0",
    "tailwindcss": "^4.1.6",
    "typescript": "^5.8.3",
    "vite": "^6.3.5"
  }
}
