{
  "name": "sveltekit-boilerplate",
  "version": "0.2.1",
  "author": "Ivo Dolenc",
  "description": "SvelteKit starter template.",
  "license": "MIT",
  "repository": "ivodolenc/sveltekit-boilerplate",
  "homepage": "https://github.com/ivodolenc/sveltekit-boilerplate",
  "private": true,
  "type": "module",
  "keywords": [
    "svelte",
    "sveltekit",
    "boilerplate",
    "template",
    "starter"
  ],
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "sync": "svelte-kit sync",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "format": "prettier --write ."
  },
  "packageManager": "pnpm@9.15.4",
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=9.0.0"
  },
  "devDependencies": {
    "@hypernym/eslint-config": "^3.5.4",
    "@hypernym/prettier-config": "^3.2.1",
    "@sveltejs/adapter-static": "^3.0.8",
    "@sveltejs/kit": "^2.16.1",
    "@sveltejs/vite-plugin-svelte": "^5.0.3",
    "@tailwindcss/vite": "^4.0.0",
    "@types/node": "^22.10.7",
    "eslint": "^9.18.0",
    "eslint-plugin-svelte": "^2.46.1",
    "prettier": "^3.4.2",
    "prettier-plugin-svelte": "^3.3.3",
    "svelte": "^5.19.2",
    "svelte-check": "^4.1.4",
    "svelte-eslint-parser": "^0.43.0",
    "tailwindcss": "^4.0.0",
    "typescript": "^5.7.3",
    "vite": "^6.0.11"
  }
}
